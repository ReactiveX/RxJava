# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY START - can be moved as a block
def j2objc_rxjava
    pod 'j2objc-rxjava-debug', :configuration => ['Debug'], :path => '../build/doppel'
    pod 'j2objc-rxjava-release', :configuration => ['Release'], :path => '../build/doppel'
    pod 'j2objc-rxjava-test', :path => '../build/j2objcOutputs'
end
# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY END

target 'ios' do
    platform :ios, '8.3'
    j2objc_rxjava
